program project1;

var
  p, q, i, s, j: integer;

begin
  s := 1;
  writeln('Введите число P ');
  readln(p);
  writeln('Введите число Q');
  readln(q);
  writeln('p := ', p, ' q := ', q);
  for i := 2 to round(sqr(q)) + 1 do // Цикл находящий делители числа Q(i)
  begin
    if q mod i = 0 then
    begin
      for j := 2 to round(sqr(i)) + 1 do // Цикл проверки на простотучисел P, i
      begin
        if (p mod j = 0) and (i mod j = 0) then
        // Если P и I делятся без остатка, то они составные
          s := 0;
      end;
      if s = 1 then // Если P и I делятся с остатком, то они простые
      begin
        writeln('Делитель числа ', q, ': ', i,
          ' - взаимно простое число с числом ', p);
      end;
      s := 1;
    end;
  end;
  readln;

end.
